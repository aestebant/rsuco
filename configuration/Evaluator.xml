<configuration>
	<evaluator name="com.uco.rs.evaluator.KFoldEvaluator">
		<computeRMSE>true</computeRMSE>
		<computeMAE>false</computeMAE>
		<computeBinaryClassification>false</computeBinaryClassification>
		<useRandomSplit>true</useRandomSplit>
		<useSpecificThreshold>true</useSpecificThreshold>
		<dataPercent>1.0</dataPercent>
		<trainPercent>0.8</trainPercent>
		<foldsNumber>5</foldsNumber>
		<reporter>
            <reportOnConsole>true</reportOnConsole>
            <reportOnFile>false</reportOnFile>
			<reportLocation>results/prueba</reportLocation>
			<reportTitle>RS</reportTitle>
		</reporter>
	</evaluator>
</configuration>